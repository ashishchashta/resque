{
	"name": "ashishchashta/resque",
	"type": "library",
	"description": "Redis backed library for creating background jobs and processing them later. Based on resque for Ruby.",
	"keywords": ["job", "background", "redis", "resque"],
	"homepage": "http://www.github.com/ashishchashta/resque/",
	"require": {
		"php": ">=5.3.0",
		"ext-pcntl": "*",
		"colinmollenhour/credis": "~1.7",
		"psr/log": "~1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "3.7.*"
	},
	"bin": [
		"bin/resque"
	],
	"autoload": {
		"psr-0": {
			"Resque": "lib"
		}
    },
	 "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
